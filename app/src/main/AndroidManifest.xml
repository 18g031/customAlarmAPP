<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alarmapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ConfigActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".activity.AlarmCreateActivity"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name=".activity.AlarmActivity1"
            android:label="@string/app_name">
        </activity>
        <receiver
            android:name="com.example.alarmapp.receiver.AlarmReceiver"
            android:permission="android.permission.Alarm"
            android:process=":remote" >
            <intent-filter>
                <action android:name="Alarm" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.example.alarmapp.receiver.AnnReceiver"
            android:permission="android.permission.Alarm"
            android:process=":remote" >
            <intent-filter>
                <action android:name="Ann" />
            </intent-filter>
        </receiver>

    </application>

</manifest>